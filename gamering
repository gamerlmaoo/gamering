if game:GetService'CoreGui':FindFirstChild'Dex' then game:GetService'CoreGui'.Dex:Destroy();endmath.randomseed(tick())local charset = {}for i = 48, 57 do table.insert(charset, string.char(i)) endfor i = 65, 90 do table.insert(charset, string.char(i)) endfor i = 97, 122 do table.insert(charset, string.char(i)) endfunction RandomCharacters(length) if length > 0 then return RandomCharacters(length - 1) .. charset[math.random(1, #charset)] else return "" endendlocal Dex = game:GetObjects("rbxassetid://3567096419")[1]Dex.Name = RandomCharacters(math.random(5, 20))Dex.Parent = game:GetService("CoreGui") local function Load(Obj, Url)local function GiveOwnGlobals(Func, Script) local Fenv = {} local RealFenv = {script = Script} local FenvMt = {} FenvMt.__index = function(a,b) if RealFenv[b] == nil then return getfenv()[b] else return RealFenv[b] end end FenvMt.__newindex = function(a, b, c) if RealFenv[b] == nil then getfenv()[b] = c else RealFenv[b] = c end end setmetatable(Fenv, FenvMt) setfenv(Func, Fenv) return Funcendlocal function LoadScripts(Script) if Script.ClassName == "Script" or Script.ClassName == "LocalScript" then spawn(function() GiveOwnGlobals(loadstring(Script.Source, "=" .. Script:GetFullName()),Script)() end) end for i,v in pairs(Script:GetChildren()) do LoadScripts(v) endendLoadScripts(Obj)
